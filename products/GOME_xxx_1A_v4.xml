    </cd:Field>
    <cd:Field name="VEADR">
      <cd:Array>
        <cd:Dimension><![CDATA[$num_veadr]]></cd:Dimension>
        <cd:NamedType id="VEADR"/>
      </cd:Array>
      <cd:Available><![CDATA[$num_veadr > 0]]></cd:Available>
